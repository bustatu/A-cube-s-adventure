    draw_set_color(trail_color)
    list_size=ds_list_size(trail_x)
    for(var i=1;i&lt;list_size;i++){
        if(trail_sprite==-1){
            draw_set_alpha(i/list_size)
            draw_line_width(trail_x[|i],trail_y[|i],trail_x[|i-1],trail_y[|i-1],trail_width)
        }
        else {draw_sprite_ext(trail_sprite,trail_sprite_index,trail_x[|i],trail_y[|i],trail_sprite_xscale*i/list_size,trail_sprite_yscale*i/list_size,0,trail_color,i/list_size)}
    }
    
    if(ds_list_size(trail_x)!=0){
        draw_set_alpha(1)
        if(trail_sprite==-1){
            draw_line_width(trail_x[i-1],trail_y[i-1],x,y,trail_width)
        }
    }
}
</string>
