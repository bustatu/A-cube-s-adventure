            <kind>1</kind>
            <string>///Step
if(!obj_gamemanager.sys_paused){
    update_rate-=delta_time/1000000
    if(!to_be_destroyed&amp;&amp;update_rate&lt;=0&amp;&amp;(x!=trail_ladded_x||y!=trail_ladded_y)){
        update_rate=update_rate_i
        ds_list_add(trail_x,x)
        ds_list_add(trail_y,y)
        
        trail_ladded_x=x
        trail_ladded_y=y
        
        if(ds_list_size(trail_x)&gt;trail_limit){
            ds_list_delete(trail_x,0)
            ds_list_delete(trail_y,0)
        }   
    }
    if(to_be_destroyed&amp;&amp;update_rate&lt;=0){
        update_rate=update_rate_i
        ds_list_delete(trail_x,0)
        ds_list_delete(trail_y,0)
        if(ds_list_size(trail_x)==0){
            instance_destroy()
        }
    }
}

</string>
