            image_angle-=rotate_speed*delta_time/1000000
        }
        else {
            image_angle+=rotate_speed*delta_time/1000000
        }
    }
    if(timer&gt;=fire_rate){
        timer%=fire_rate
        randomize()
        if(random(101)%101&lt;=90){
            inst=instance_create(x,y,obj_ebullet)
            inst.direction=image_angle
            inst.image_angle=image_angle
            inst.image_blend=c_yellow     
        }
        else {
            with(instance_create(x,y,obj_erocket)){
                direction=point_direction(x,y,obj_plr.x,obj_plr.y)
                image_angle=direction
            }        
        }
    }
    visible=true
}
else if(!active||obj_gamemanager.sys_paused){visible=false}
