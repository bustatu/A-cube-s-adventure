            <kind>1</kind>
            <string>///Step

///Cursor visibility
if(instance_exists(obj_plr)&amp;&amp;instance_exists(obj_pausebutton)&amp;&amp;!obj_pausebutton.game_paused){
    if(obj_plr.moving&amp;&amp;l&amp;&amp;(obj_plr.target_x==x&amp;&amp;obj_plr.target_y==y)){visible=false}
    else {visible=ivisible}
}
else {visible=ivisible}

///Control cursor
if(obj_gamemanager.gamepad_con==0){
    x=mouse_x
    y=mouse_y
    if(x&lt;=view_xview[0]){x=view_xview[0]}
    if(y&lt;=view_yview[0]){y=view_yview[0]}
    if(x&gt;=view_xview[0]+view_wview[0]){x=view_xview[0]+view_wview[0]}
    if(y&gt;=view_yview[0]+view_hview[0]){y=view_yview[0]+view_hview[0]}
    if(mouse_check_button(mb_left)&amp;&amp;mouse_check_button(mb_right)){image_index=3}
    else if(mouse_check_button(mb_left)){image_index=1}
    else if(mouse_check_button(mb_right)){image_index=2}
    else {image_index=0}
}
else {
    for(var i=0;i&lt;obj_gamemanager.gamepad_num;i++){
        if(obj_gamemanager.gamepad[i]){
            x+=controller_sensivity*gamepad_axis_value(i,gp_axislh)*delta_time/1000000
            y+=controller_sensivity*gamepad_axis_value(i,gp_axislv)*delta_time/1000000
            if(x&lt;=view_xview[0]){x=view_xview[0]}
            if(y&lt;=view_yview[0]){y=view_yview[0]}
            if(x&gt;=view_xview[0]+view_wview[0]){x=view_xview[0]+view_wview[0]}
            if(y&gt;=view_yview[0]+view_hview[0]){y=view_yview[0]+view_hview[0]}
            if(gamepad_button_check(i,gp_face1)&amp;&amp;gamepad_button_check(i,gp_face4)){image_index=3}
            else if(gamepad_button_check(i,gp_face1)){image_index=1}
            else if(gamepad_button_check(i,gp_face4)){image_index=2}
            else {image_index=0}
        }
    }
}

///Get input values
if(obj_gamemanager.gamepad_con==0){
    if(mouse_check_button_pressed(mb_left)){lpressed=true}
    else {lpressed=false}
    if(mouse_check_button(mb_left)){l=true}
    else {l=false}
    if(mouse_check_button_released(mb_left)){lreleased=true}
    else {lreleased=false}
    if(mouse_check_button_pressed(mb_right)){rpressed=true}
    else {rpressed=false}
    if(mouse_check_button(mb_right)){r=true}
    else {r=false}
    if(mouse_check_button_released(mb_right)){rreleased=true}
    else {rreleased=false}
}
else {
    for(var i=0;i&lt;obj_gamemanager.gamepad_num;i++){
        if(obj_gamemanager.gamepad[i]){
            if(gamepad_button_check_pressed(i,gp_face1)){lpressed=true}
            else {lpressed=false}
            if(gamepad_button_check(i,gp_face1)){l=true}
            else {l=false}
            if(gamepad_button_check_released(i,gp_face1)){lreleased=true}
            else {lreleased=false}
            if(gamepad_button_check_pressed(i,gp_face4)){rpressed=true}
            else {rpressed=false}
            if(gamepad_button_check(i,gp_face4)){r=true}
            else {r=false}
            if(gamepad_button_check_released(i,gp_face4)){rreleased=true}
            else {rreleased=false}        
        }
    }
}
</string>
